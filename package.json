    "express": "^4.16.2",
    "express-fileupload": "^0.4.0",
    "express-static": "^1.2.5",
    "mongoose": "^5.2.6",
    "nodemailer": "^4.6.7",
    "react-router-dom": "^4.3.1"
  }
